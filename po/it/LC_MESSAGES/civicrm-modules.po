#: templates/CRM/Grant/Form/Search/EmptyResults.tpl
#: templates/CRM/Member/Form/Search/EmptyResults.tpl
msgid "No matching memberships found."
msgstr "Nessuna iscrizione corrispondente trovata."

#: templates/CRM/Event/Form/Search/EmptyResults.tpl
msgid "if you are searching by participant name, check your spelling"
msgstr "Se state facendo una ricerca per nome del partecipante, controllate l'ortografia"

#: templates/CRM/Event/Form/SearchEvent.tpl
