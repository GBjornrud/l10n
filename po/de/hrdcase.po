#: templates/CRM/HRDCase/Form/CaseActivity.tpl
msgid "Regarding"
msgstr "Betreffend"

#: templates/CRM/HRDCase/Form/Case.tpl
msgid "New Case"
msgstr "Neuer Fall"

#: templates/CRM/HRDCase/Form/Case.tpl
